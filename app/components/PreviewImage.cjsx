React = require "react"
SelectionStorage = require "../storages/SelectionStorage"

class PreviewImage extends React.Component

  constructor: (props) ->
    super props
    @state =
      fsObject: SelectionStorage.getLastAdded()

  componentDidMount: () ->
    SelectionStorage.addUpdateListener(@_onChange.bind @)

  render: () ->
    src = "data:image/png;base64,"
    if @state.fsObject? && @state.fsObject.mimeType?
      if @state.fsObject.mimeType.match /^image\//
        src = decodeURIComponent @state.fsObject.path
    return (
      <img src={src} />
    )

  _onChange: () ->
    console.log "AASD"
    @setState
      fsObject: SelectionStorage.getLastAdded()


module.exports = PreviewImage